import Vec

type String = 
    vec: Vec char

empty () = String (empty ())

reserve sb s =
    reserve sb.vec s.length

append sb s = 
    reserve sb s
    memcpy sb.vec.data s.c_string s.length // use some sort of extend trait

impl Print String with
    print s =
        print (string s.vec.data s.vec.length)
