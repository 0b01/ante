fun printMsg: bool b
    if b -> puts("Hello World!")
    else -> puts("Goodbye World!")


fun divisibleByThree: i32 i => bool
    return i % 3 == 0


fun isEven: i32 x => bool
    return x % 2 == 0


fun fact: i32 x => i32
    if x <= 1
        return 1
    return x * fact(x-1)


printMsg(true)
if divisibleByThree(23)
   printf("23 is divisible by 3\n")

if isEven(4)
   printf("fact(5) = %d\n", fact(5))

printMsg(1 + 1 != 2)

