fun printMsg: bool b
    if b then
        puts "Hello World!"
    else
        puts "Goodbye World!"


fun divisibleByThree: i32 i => bool
    i % 3 == 0


fun isEven: i32 x => bool
    x % 2 == 0


fun fact: i32 x => i32
    if x <= 1 then 1
    else x * fact(x-1)


printMsg true
if divisibleByThree 23 then
   puts "23 is divisible by 3"

if isEven 4 then
   printf "fact(5) = %d\n" (fact 5)

printMsg(1 + 1 != 2)

