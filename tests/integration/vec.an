import Vec

mut v = Vec<i32>()
v.push 1
v.push 1
v.push 2
v.push 3
v.push 5
v.push 8
v.push 13
v.push 21

fun assert: bool b
    if not b then
        puts "Assertation failed"
        exit 1

v.pop()

assert (13 in v)
assert (21 not in v)

let idx4 = v#4
v.remove_index 4
assert (idx4 not in v)

//remove element 8 at index 4
let idx = v.remove_first 8
assert (8 not in v)
assert (idx == Some 4)

let count = v.remove_all 1
assert (1 not in v)
assert (count == 2)
 
mut v2 = Vec<i32>()
v2.push 2
v2.push 3
v2.push 13

assert (v == v2)


