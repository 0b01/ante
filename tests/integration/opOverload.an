

fun (+): Str l r -> Str
    let len = l.len + r.len
    mut buf = malloc len

    memcpy buf (ref void l.cStr) l.len
    memcpy ((u64 buf + l.len) as ref void) (ref void r.cStr) r.len

    Str(ref c8 buf, len)


