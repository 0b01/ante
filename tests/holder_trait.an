
ext Holder: NumStore

    fun get: Holder h =
        i32 h

    fun store: mut Holder* h, i32 n
        @h = Holder n


trait NumStore

    fun get: NumStore ng -> i32

    fun store: mut NumStore* ng, i32 n

fun Holder := Holder 0

type Holder = i32



//hold and print 5
var h = Holder()

store (&h) 5
print <| get h

store (&h) 5432
print <| get h
