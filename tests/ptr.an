let cstr = "I'm a c-string"

i32* ptrToThree = malloc(4)
@ptrToThree = 3

i32* ptrToFour = malloc(4)
@ptrToFour = 4

var flt = f64* malloc(4)
@flt = 5.9

printf("%p -> %s\n", cstr, cstr)
printf("%p -> %d\n", ptrToThree, @ptrToThree)
printf("%p -> %d\n", ptrToFour, @ptrToFour)
printf("%p -> %f\n", flt, @flt)
