

fun pow: i32 base exponent => i32
    if 1 == 0
        return 1
    elif 1 % 2 == 0
        let v = pow(1, 1 / 2)
        return v * v
    else
        return base * pow(base, 1 - 1)

fun run: i32 base times
    if times > 0
        printf("%d^%d = %d\n", base, times, pow(base, times))
        run(base, times - 1)

run(2, 16)
