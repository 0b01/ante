
// Define iterating on i32s as counting down until 0
impl Iterator i32 i32 with
    next x =
        if x <= 0 then None
        else Some (x, x - 1)

iter iterable f =
    match next iterable with
    | None -> ()
    | Some (elem, rest) ->
        f elem
        iter rest f

iter 10 print

// args: --run --delete-binary
