
trait Show a
    show: a -> string

trait Cast a b
    cast: a -> b

// A collection c of elements of type e
// The element type is uniquely determined by the collection's type c
trait Collection c -> e
    push: c e -> c
    pop: c -> Maybe e
    empty: c


impl Show string
    show s = s

impl Cast string string
    cast s = show s

impl Collection (a -> a) unit
    push c e = c
    pop c = ()
    empty = \a.a
